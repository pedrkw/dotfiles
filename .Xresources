! Black + DarkGrey
*color0:  #282c34
*color8:  #545862
! DarkRed + Red
*color1:  #e06c75
*color9:  #e06c75
! DarkGreen + Green
*color2:  #98c379
*color10: #98c379
! DarkYellow + Yellow
*color3:  #e5c07b
*color11: #e5c07b
! DarkBlue + Blue
*color4:  #61afef
*color12: #61afef
! DarkMagenta + Magenta
*color5:  #c678dd
*color13: #c678dd
!DarkCyan + Cyan (both not tango)
*color6:  #56b6c2
*color14: #56b6c2
! LightGrey + White
*color7:  #abb2bf
*color15: #c8ccd4

! ! Set depth to make transparency work.
! URxvt*depth: 32

XTerm*faceName: Source Code Pro Medium
XTerm*faceSize: 11
XTerm*saveLines: 16384
XTerm*log0inShell: true
XTerm*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
XTerm*termName: xterm-256color
XTerm*highlightSelection: true
XTerm**trimSelection: true
XTerm.vt100.translations: #override \n\
	Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
	Ctrl Shift <Key>V: insert-selection(CLIPBOARD) \n\
	<Btn1Up>: select-end(PRIMARY, CLIPBOARD, CUT_BUFFER0) \n\
	<Btn2Up>: insert-selection(PRIMARY)
XTerm*selectTOClipboard: true
XTerm*metaSendsEscape: true
Xterm*internalBorder: 80

Xcursor.size: 11

UXTerm*faceName: Source Code Pro Medium
UXTerm*faceSize: 11
UXTerm*saveLines: 16384
UXTerm*log0inShell: true
UXTerm*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
UXTerm*termName: xterm-256color
UXTerm*highlightSelection: true
UXTerm**trimSelection: true
UXTerm.vt100.translations: #override \n\
 Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
 Ctrl Shift <Key>V: insert-selection(CLIPBOARD) \n\
 <Btn1Up>: select-end(PRIMARY, CLIPBOARD, CUT_BUFFER0) \n\
 <Btn2Up>: insert-selection(PRIMARY)
UXTerm*selectTOClipboard: true
UXTerm*metaSendsEscape: true
UXterm*internalBorder: 80

UXterm*cursor.size: 12

URxvt*font: xft:Source Code Pro:pixelsize=13:antialiasing=true:size=12
URxvt*letterspace: 0
URxvt*scrollBar: false
URxvt.intensityStyles: false
URxvt.transparent:   false
URxvt*.shading: 2
URxvt.depth: 24
URxvt.cursorUnderline: true
URxvt.termName: rxvt-256color
URxvt.buffered: false
URxvt*fading:30

URxvt.cursorColor:  #e06c75
URxvt.pointerColor: #61afef
URxvt.background:   #282c34
URxvt.foreground:   #c8ccd4
URxvt.colorUL: #abb2bf

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

URxvt.perl-ext-common: default,matcher,fullscreen,clipboard,resize-font
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.keysym.F11: perl:fullscreen:switch
URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up
URxvt.secondaryWheel: 1
URxvt.internalBorder:  5
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False
URxvt.keysym.Control-Up:   \033[1;5A
URxvt.keysym.Control-Down:    \033[1;5B
URxvt.keysym.Control-Left:    \033[1;5D
URxvt.keysym.Control-Right:    \033[1;5C

URxvt.tabbed.tabbar-fg: 4
URxvt.tabbed.tabbar-bg: 16
URxvt.tabbed.tab-fg:    15
URxvt.tabbed.tab-bg:    4
URxvt*buffered:         false

!URxvt.tab-bg: 233
!URxvt.tab-fg: 8
!URxvt.tabbar-bg: 233
!URxvt.tabbar-fg: 240

